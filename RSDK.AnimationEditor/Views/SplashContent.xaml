        <Image x:Name="Image" Source="/Assets/SmallIcon.Ico" HorizontalAlignment="Center" VerticalAlignment="Center" MinWidth="32" MinHeight="32" MaxWidth="96" MaxHeight="96" Margin="126,0"/>
    </Grid>
</Page>
